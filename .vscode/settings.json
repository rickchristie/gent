{
  "editor.rulers": [100],
  "go.testEnvFile": "${workspaceFolder}/.env",
  "go.testFlags": ["-v", "-count=1"],
  "go.testTimeout": "15m",
  "terminal.integrated.profiles.linux": {
    "bash-env": {
      "path": "bash",
      "args": ["--rcfile", "${workspaceFolder}/.vscode/bashrc"]
    }
  },
  "terminal.integrated.defaultProfile.linux": "bash-env"
}
